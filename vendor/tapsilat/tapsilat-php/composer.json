{
  "name": "tapsilat/tapsilat-php",
  "description": "Client SDK for Tapsilat API",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "tapsilat",
    "api",
    "sdk",
    "client"
  ],
  "homepage": "https://github.com/tapsilat/tapsilat-php",
  "support": {
    "issues": "https://github.com/tapsilat/tapsilat-php/issues",
    "source": "https://github.com/tapsilat/tapsilat-php"
  },
  "authors": [
    {
      "name": "Tapsilat",
      "email": "support@tapsilat.dev"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "scripts": {
    "test": "phpunit"
  },
  "autoload": {
    "psr-4": {
      "Tapsilat\\": "src/",
      "Tapsilat\\Models\\": "src/Models/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tapsilat\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
