{
    "name": "tapsilat/tapsilat-woocommerce",
    "description": "WooCommerce Plugin for Tapsilat Payment Gateway",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Tapsilat\\WooCommerce\\": "includes/",
            "Tapsilat\\": "libs/tapsilat-php/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}